{
  "profiles": {
    "run": {
      "commandName": "Executable",
      "executablePath": "docker",
      "commandLineArgs": "compose up",
      "workingDirectory": "."
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:5001;http://+:5000",
        "ASPNETCORE_HTTPS_PORT": "5001"
      },
      "httpPort": 80,
      "useSSL": false,
      "sslPort": 443
    }
  }
}